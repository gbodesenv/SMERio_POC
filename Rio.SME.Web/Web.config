  <system.web>
    <!--__________SISTEMA NOVO ESCOLA 3.0____________-->
    <!--Diagnóstico-->
    <trace enabled="false" pageOutput="false" mostRecent="true" localOnly="true" />
    <!-- Segurança -->
    <!--<trust level="Minimal"/>-->
    <machineKey validation="SHA1" decryption="AES" validationKey="5ADDE444FB8D869B71A51BAEAF5FA4E3FD12BD4A4C25538259D6FC3DC918CDF8" decryptionKey="5C550FBD21E3BD516AFA39615D389615" />
    <!--impersonate="false" para funcionar o login no AD-->
    <identity impersonate="false" />
    <httpCookies httpOnlyCookies="true" />
    <authentication mode="Forms">
      <!--forms Cada "name" deve ser diferente para cada site-->
      <forms name="930DCAE5-D473-4f60-B91A-55E6632D591C" cookieless="UseCookies" timeout="120" protection="All" slidingExpiration="true" loginUrl="logout.ashx" enableCrossAppRedirects="false" path="/" />
    </authentication>
    <!--sessionState Cada cookieName deve ser diferente para cada site-->
    <sessionState cookieName="DF696E5B-5B19-4f6f-9B2B-28DA5B7837A5" cookieless="UseCookies" timeout="30" mode="InProc" />

    <siteMap defaultProvider="SsoSiteMapProvider" enabled="true">
      <providers>
        <clear />
        <add name="SsoSiteMapProvider" description="Default SiteMap provider." type="MSTech.CoreSSO.Providers.SsoSiteMapProvider, MSTech.CoreSSO.Providers" securityTrimmingEnabled="true" urlRoot="~/Index.aspx" sistemaID="2" />
      </providers>
    </siteMap>

    <pages theme="Rio">
      <controls>
        <add tagPrefix="asp" namespace="System.Web.UI" assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add tagPrefix="asp" namespace="System.Web.UI.WebControls" assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      </controls>
    </pages>    
    <!--__________SISTEMA NOVO ESCOLA 3.0____________-->


    <!--<globalization uiCulture="pt" culture="pt-BR" />-->
    <globalization uiCulture="pt-BR" culture="pt-BR" fileEncoding="iso-8859-1" responseHeaderEncoding="iso-8859-1" requestEncoding="utf-8" responseEncoding="utf-8" />
    <compilation debug="true" targetFramework="4.7" />
    <httpRuntime targetFramework="4.5" executionTimeout="240" maxRequestLength="20480" />

    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
      <!--__________SISTEMA NOVO ESCOLA 3.0____________-->
      <add name="ScriptModule" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
    </httpModules>
  </system.web>

  <!--__________SISTEMA NOVO ESCOLA 3.0____________-->
  <ServiceProvider id="http://localhost:57602/SAML/" xmlns="urn:MSTech.SAML20.Configuration">
